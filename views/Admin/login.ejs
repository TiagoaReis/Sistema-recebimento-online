<!DOCTYPE html>
<html>
  
  <%- include("Includes/head.ejs") -%>

<body class="hold-transition login-page">
  <div class="login-box well well-shadow">
    <div class="login-logo" >
      <!-- <a href="#"><img src="dist/img/gest/02.png" alt="" class="img-responsive" style="width: auto; height: auto"></a> -->
      <a href="#"><img src="dist/img/gest/logo01.png" alt="" class="img-responsive" style="width: auto; height: auto"></a>
    </div>
    <!-- /.login-logo -->
    <div class="login-box-body">
      <h4 class="login-box-msg">Acesso a Ã¡rea restrita</h4>
      <% if (typeof error === "string") {%>
        <div class="alert alert-danger">
          <%= error%>
        </div>
        <% } %>
      <form method="post">
        <div class="form-group has-feedback">
          <input name= "email" type="email" class="form-control" placeholder="E-mail">
          <span class="glyphicon glyphicon-envelope form-control-feedback"></span>
        </div>
        <div class="form-group has-feedback">
          <input name= "senha" type="password" class="form-control" placeholder="Senha">
          <span class="glyphicon glyphicon-lock form-control-feedback"></span>
        </div>
        <div class="row">
          <!-- /.col -->
          <div class="col-xs-12">
            <button type="submit" class="btn btn-lg btn-primary btn-block"><i class="fa fa-sign-in">  </i>  Entrar</button>
          </div>
          <!-- /.col -->
        </div>
      </form>

    </div>
    <!-- /.login-box-body -->
  </div>
  <!-- /.login-box -->

  <!-- jQuery 3 -->
  <script src="bower_components/jquery/dist/jquery.min.js"></script>
  <!-- Bootstrap 3.3.7 -->
  <script src="bower_components/bootstrap/dist/js/bootstrap.min.js"></script>

  
  
</body>

</html>
